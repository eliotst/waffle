<h1>Log in</h1>

<%= form_tag users_path do %>
	<p class="email">
		<%= label_tag :email %><br />
		<%= text_field_tag :email, params[:email] %>
	</p>
	<p class="password">
		<%= label_tag :password %><br />
		<%= password_field_tag :password %>
	</p>
	<p class="login_button"><%= submit_tag "Log in" %></p>
<% end %>