<div class="fields">
  <p>
    <%= f.label :label, "Questionnaire Label" %>
    <%= f.text_area :label, :rows => 1 %>
    <%= link_to_remove_fields "Remove Questionnaire", f%><br/>
    <%= f.fields_for :blocks do |builder| %>
      <%= render 'block_fields', :f => builder %>
    <% end %>
    <p><%= link_to_add_fields "Add Block", f, :blocks %></p>
  </p>
</div>