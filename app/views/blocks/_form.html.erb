<%= form_for @block do |f| %>
  <p>
    <%= f.label :label %>
    <%= f.text_field :label %>
  </p>
  <%= f.fields_for :questions do |builder| %>
  	<%= render 'question_fields', :f => builder %>
  <% end %>
  <p><%= f.submit "Submit block", class: "Center btn btn-large btn-primary" %>
  <%= link_to "List of Current Blocks", blocks_path, class: "btn btn-large btn-link" %></p>
<% end %>
